{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "pretty": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "Components": [
        "./src/Components"
      ],
      "Audio": [
        "./src/Components/Audio"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "webpack"
  ],
}
